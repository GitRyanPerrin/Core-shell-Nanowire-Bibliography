	author = {Black, L. E. and Cavalli, A. and Verheijen, M. A. and Haverkort, J. E. M. and Bakkers, E. P. A. M. and Kessels, W. M. M.},
	month = oct,
	year = {2017},
	note = {Publisher: American Chemical Society},
	pages = {6287--6294},
	file = {ACS Full Text Snapshot:C\:\\Users\\Ryan\\Zotero\\storage\\4J5YIKV9\\acs.nanolett.html:text/html;Full Text PDF:C\:\\Users\\Ryan\\Zotero\\storage\\PPKEFQEZ\\Black et al. - 2017 - Effective Surface Passivation of InP Nanowires by .pdf:application/pdf},
}

